<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/ImageSharing.DAL.EntityNew" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/ImageSharing.DAL.EntityNew" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="FriendshipRequest">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:Entity">
